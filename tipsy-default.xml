<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="tipsy"
           fetch="git://github.com/TipsyOs"
           revision="10.0"
           sync-c="true"
           sync-j="8" />

  <!-- Tipsy repos -->
  <project path="bionic" name="android_bionic" groups="pdk" remote="tipsy" />
  <project path="build/make" name="tipsy_build" groups="pdk" remote="tipsy">
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="build_soong" groups="pdk,tradefed" remote="tipsy" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  <project path="bootable/recovery" name="bootable_recovery" groups="pdk" remote="tipsy" />
  <project path="external/tinycompress" name="external_tinycompress" groups="pdk" remote="tipsy" />
  <project path="frameworks/av" name="frameworks_av" groups="pdk" remote="tipsy" />
  <project path="frameworks/base" name="frameworks_base" groups="pdk-cw-fs,pdk-fs" remote="tipsy" />
  <project path="frameworks/native" name="frameworks_native" groups="pdk" remote="tipsy" />
  <project path="frameworks/rs" name="frameworks_rs" groups="pdk" remote="tipsy" />
  <project path="hardware/interfaces" name="hardware_interfaces" groups="pdk" remote="tipsy" />
  <project path="hardware/lineage/interfaces" name="hardware_lineage_interfaces" remote="tipsy" />
  <project path="packages/apps/MartysPlace" name="packages_apps_MartysPlace" remote="tipsy" />
  <project path="packages/apps/Settings" name="packages_apps_Settings" groups="pdk-fs" remote="tipsy" />
  <project path="packages/apps/TipsySetupWizard" name="packages_apps_TipsySetupWizard" remote="tipsy" />
  <project path="system/core" name="android_system_core" groups="pdk" remote="tipsy" />
  <project path="system/vold" name="system_vold" groups="pdk" remote="tipsy" />
  <project path="vendor/qcom/opensource/data-ipa-cfg-mgr" name="vendor_qcom_opensource_data-ipa-cfg-mgr" remote="tipsy" groups="qcom,pdk-qcom" />
  <project path="vendor/tipsy" name="vendor_tipsy" remote="tipsy" />

</manifest>
